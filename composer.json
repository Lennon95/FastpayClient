{
    "name": "lennon95/fastpay-client",
    "description": "Client de consulta API da Fastpay",
    "keywords": [
        "fastpay",
        "rest",
        "psr7"
    ],
    "homepage": "http://github.com/Lennon95/FastpayClient",
    "license": "MIT",
    "authors": [
        {
            "name": "Lennon da Silva",
            "email": "lennon.silva95@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Fastpay\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
